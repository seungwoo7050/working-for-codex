{
  "dashboard": {
    "title": "native-video-editor - Phase 2: Native Video Processing",
    "tags": ["video-editor", "phase2", "video", "native", "cpp"],
    "timezone": "browser",
    "schemaVersion": 27,
    "version": 1,
    "refresh": "5s",
    "panels": [
      {
        "id": 1,
        "gridPos": { "x": 0, "y": 0, "w": 12, "h": 8 },
        "type": "graph",
        "title": "Thumbnail Extraction Performance",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum(rate(video_editor_thumbnail_duration_seconds_bucket[5m])) by (le))",
            "legendFormat": "p50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(video_editor_thumbnail_duration_seconds_bucket[5m])) by (le))",
            "legendFormat": "p95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, sum(rate(video_editor_thumbnail_duration_seconds_bucket[5m])) by (le))",
            "legendFormat": "p99 (Target: < 0.05s)",
            "refId": "C"
          }
        ],
        "yaxes": [
          {
            "format": "s",
            "label": "Duration",
            "logBase": 1,
            "min": 0
          },
          {
            "format": "short"
          }
        ],
        "thresholds": [
          {
            "value": 0.05,
            "colorMode": "critical",
            "op": "gt",
            "fill": true,
            "line": true
          }
        ],
        "alert": {
          "name": "Thumbnail p99 Latency High",
          "message": "Thumbnail extraction p99 latency exceeds 50ms",
          "conditions": [
            {
              "evaluator": {
                "params": [0.05],
                "type": "gt"
              },
              "operator": {
                "type": "and"
              },
              "query": {
                "params": ["C", "5m", "now"]
              },
              "reducer": {
                "params": [],
                "type": "avg"
              },
              "type": "query"
            }
          ]
        }
      },
      {
        "id": 2,
        "gridPos": { "x": 12, "y": 0, "w": 12, "h": 8 },
        "type": "graph",
        "title": "Metadata Extraction Performance",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum(rate(video_editor_metadata_duration_seconds_bucket[5m])) by (le))",
            "legendFormat": "p50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(video_editor_metadata_duration_seconds_bucket[5m])) by (le))",
            "legendFormat": "p95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, sum(rate(video_editor_metadata_duration_seconds_bucket[5m])) by (le))",
            "legendFormat": "p99 (Target: < 0.1s)",
            "refId": "C"
          }
        ],
        "yaxes": [
          {
            "format": "s",
            "label": "Duration",
            "min": 0
          }
        ],
        "thresholds": [
          {
            "value": 0.1,
            "colorMode": "critical",
            "op": "gt"
          }
        ]
      },
      {
        "id": 3,
        "gridPos": { "x": 0, "y": 8, "w": 8, "h": 6 },
        "type": "stat",
        "title": "Thumbnail Request Rate",
        "targets": [
          {
            "expr": "sum(rate(video_editor_thumbnail_requests_total[5m]))",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value",
          "justifyMode": "center",
          "textMode": "value_and_name",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          }
        },
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": 0, "color": "green" },
                { "value": 10, "color": "yellow" },
                { "value": 50, "color": "red" }
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "gridPos": { "x": 8, "y": 8, "w": 8, "h": 6 },
        "type": "stat",
        "title": "Thumbnail Cache Hit Ratio",
        "targets": [
          {
            "expr": "video_editor_thumbnail_cache_hit_ratio",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value",
          "textMode": "value_and_name"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": 0, "color": "red" },
                { "value": 0.5, "color": "yellow" },
                { "value": 0.8, "color": "green" }
              ]
            },
            "min": 0,
            "max": 1
          }
        }
      },
      {
        "id": 5,
        "gridPos": { "x": 16, "y": 8, "w": 8, "h": 6 },
        "type": "stat",
        "title": "Metadata Request Rate",
        "targets": [
          {
            "expr": "sum(rate(video_editor_metadata_requests_total[5m]))",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "reqps"
          }
        }
      },
      {
        "id": 6,
        "gridPos": { "x": 0, "y": 14, "w": 12, "h": 8 },
        "type": "graph",
        "title": "Error Rates",
        "targets": [
          {
            "expr": "sum(rate(video_editor_thumbnail_errors_total[5m])) by (error_type)",
            "legendFormat": "Thumbnail - {{error_type}}",
            "refId": "A"
          },
          {
            "expr": "sum(rate(video_editor_metadata_errors_total[5m])) by (error_type)",
            "legendFormat": "Metadata - {{error_type}}",
            "refId": "B"
          },
          {
            "expr": "sum(rate(video_editor_ffmpeg_errors_total[5m])) by (operation)",
            "legendFormat": "FFmpeg - {{operation}}",
            "refId": "C"
          }
        ],
        "yaxes": [
          {
            "format": "short",
            "label": "Errors/sec",
            "min": 0
          }
        ]
      },
      {
        "id": 7,
        "gridPos": { "x": 12, "y": 14, "w": 12, "h": 8 },
        "type": "graph",
        "title": "Memory Usage",
        "targets": [
          {
            "expr": "video_editor_memory_usage_bytes{type=\"rss\"}",
            "legendFormat": "RSS",
            "refId": "A"
          },
          {
            "expr": "video_editor_memory_usage_bytes{type=\"heap_used\"}",
            "legendFormat": "Heap Used",
            "refId": "B"
          },
          {
            "expr": "video_editor_memory_usage_bytes{type=\"heap_total\"}",
            "legendFormat": "Heap Total",
            "refId": "C"
          }
        ],
        "yaxes": [
          {
            "format": "bytes",
            "label": "Memory",
            "min": 0
          }
        ],
        "thresholds": [
          {
            "value": 524288000,
            "colorMode": "critical",
            "op": "gt",
            "fill": true,
            "line": true
          }
        ]
      },
      {
        "id": 8,
        "gridPos": { "x": 0, "y": 22, "w": 24, "h": 8 },
        "type": "graph",
        "title": "API Latency by Endpoint",
        "targets": [
          {
            "expr": "histogram_quantile(0.99, sum(rate(video_editor_api_latency_seconds_bucket[5m])) by (route, le))",
            "legendFormat": "{{route}} - p99",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(video_editor_api_latency_seconds_bucket[5m])) by (route, le))",
            "legendFormat": "{{route}} - p95",
            "refId": "B"
          }
        ],
        "yaxes": [
          {
            "format": "s",
            "label": "Latency",
            "min": 0
          }
        ],
        "thresholds": [
          {
            "value": 0.2,
            "colorMode": "critical",
            "op": "gt"
          }
        ]
      },
      {
        "id": 9,
        "gridPos": { "x": 0, "y": 30, "w": 12, "h": 6 },
        "type": "table",
        "title": "Performance KPIs",
        "targets": [
          {
            "expr": "histogram_quantile(0.99, sum(rate(video_editor_thumbnail_duration_seconds_bucket[5m])) by (le))",
            "format": "time_series",
            "legendFormat": "Thumbnail p99",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.99, sum(rate(video_editor_metadata_duration_seconds_bucket[5m])) by (le))",
            "legendFormat": "Metadata p99",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, sum(rate(video_editor_api_latency_seconds_bucket[5m])) by (le))",
            "legendFormat": "API p99",
            "refId": "C"
          },
          {
            "expr": "video_editor_memory_usage_bytes{type=\"rss\"} / 1024 / 1024",
            "legendFormat": "Memory (MB)",
            "refId": "D"
          }
        ],
        "transformations": [
          {
            "id": "seriesToRows",
            "options": {}
          }
        ],
        "options": {
          "showHeader": true
        },
        "fieldConfig": {
          "overrides": [
            {
              "matcher": { "id": "byName", "options": "Value" },
              "properties": [
                {
                  "id": "custom.displayMode",
                  "value": "color-background"
                },
                {
                  "id": "thresholds",
                  "value": {
                    "mode": "absolute",
                    "steps": [
                      { "value": 0, "color": "green" },
                      { "value": 0.05, "color": "yellow" },
                      { "value": 0.1, "color": "red" }
                    ]
                  }
                }
              ]
            }
          ]
        }
      },
      {
        "id": 10,
        "gridPos": { "x": 12, "y": 30, "w": 12, "h": 6 },
        "type": "stat",
        "title": "System Status",
        "targets": [
          {
            "expr": "sum(rate(video_editor_thumbnail_requests_total{status=\"success\"}[5m])) / sum(rate(video_editor_thumbnail_requests_total[5m]))",
            "legendFormat": "Thumbnail Success Rate",
            "refId": "A"
          },
          {
            "expr": "sum(rate(video_editor_metadata_requests_total{status=\"success\"}[5m])) / sum(rate(video_editor_metadata_requests_total[5m]))",
            "legendFormat": "Metadata Success Rate",
            "refId": "B"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": 0, "color": "red" },
                { "value": 0.95, "color": "yellow" },
                { "value": 0.99, "color": "green" }
              ]
            },
            "min": 0,
            "max": 1
          }
        }
      }
    ],
    "time": {
      "from": "now-30m",
      "to": "now"
    }
  }
}
